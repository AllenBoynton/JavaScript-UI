// Background color
Ti.UI.setBackgroundColor("#000");

// Background
var mainWindow = Ti.UI.createWindow ({
	backgroundColor: "#fff",
	title: "ABOUT ME"
});

// Convert to navigation window
var navWindow = Ti.UI.iOS.createNavigationWindow ({
	window: mainWindow
});


